{
  "name": "@expo/expo",
  "version": "1.4.0",
  "private": true,
  "author": "Expo",
  "license": "MIT",
  "scripts": {
    "setup:docs": "./scripts/download-dependencies.sh",
    "setup:native": "./scripts/download-dependencies.sh && ./scripts/setup-react-android.sh",
    "postinstall": "yarn-deduplicate && expo-yarn-workspaces check-workspace-dependencies && yarn workspace @expo/cli prepare",
    "lint": "eslint .",
    "tsc": "echo 'You are trying to run \"tsc\" in the workspace root. Run it from an individual package instead.' && exit 1"
  },
  "workspaces": {
    "packages": [
      "apps/*",
      "apps/test-suite/puppeteer-test",
      "home",
      "packages/*",
      "packages/@expo/*"
    ]
  },
  "resolutions": {
    "react-native": "0.70.5",

    "metro": "0.73.5",
    "metro-babel-transformer": "0.73.5",
    "metro-cache": "0.73.5",
    "metro-file-map": "0.73.5",
    "metro-hermes-compiler": "0.73.5",
    "metro-inspector-proxy": "0.73.5",
    "metro-react-native-babel-transformer": "0.73.5",
    "metro-resolver": "0.73.5",
    "metro-runtime": "0.73.5",
    "metro-source-map": "0.73.5",
    "metro-symbolicate": "0.73.5",
    "metro-transform-plugins": "0.73.5",
    "metro-transform-worker": "0.73.5",
    "metro-cache-key": "0.73.5",
    "metro-config": "0.73.5",
    "metro-core": "0.73.5",
    "metro-minify-terser": "0.73.5",
    "metro-minify-uglify": "0.73.5",
    "metro-react-native-babel-preset": "0.73.5",

    "@react-navigation/elements": "^1.3.6",
    "@react-navigation/bottom-tabs": "^6.3.3",
    "@react-navigation/native": "^6.0.12",
    "@react-navigation/native-stack": "^6.8.0",
    "@react-navigation/core": "^6.4.6",

    "**/util": "~0.12.4"
  },
  "dependencies": {
    "eslint": "^8.29.0",
    "expo-yarn-workspaces": "*",
    "jsc-android": "^250230.2.1",
    "prettier": "^2.8.1",
    "yarn-deduplicate": "^6.0.0"
  }
}
